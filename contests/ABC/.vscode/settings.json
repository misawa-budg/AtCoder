{
    "files.associations": {
        "*.dbclient-js": "javascript",
        "ostream": "cpp"
    },
    "code-runner.executorMap": {
        "cpp": "cd $dir && g++ -std=gnu++23 -O2 -Wall -Wextra -Wno-sign-compare -fdiagnostics-color=always -I$workspaceRoot/../.. \"$fileName\" -o \"$fileNameWithoutExt\" && ./$fileNameWithoutExt"
    },
    "C_Cpp.default.compilerPath": "/usr/bin/g++",
    "C_Cpp.default.cppStandard": "gnu++23",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/../..",
        "${workspaceFolder}/../..//**"
    ],
    "C_Cpp.errorSquiggles": "Disabled"
}
