{
  "code-runner.executorMap": {
    "cpp": "cd $dir && g++ -std=gnu++20 -O2 -Wall -Wextra -fdiagnostics-color=always -I$workspaceRoot \"$fileName\" -o \"$fileNameWithoutExt\" && ./$fileNameWithoutExt",
    "c": "cd $dir && gcc -std=c17 -O2 -Wall -Wextra -fdiagnostics-color=always \"$fileName\" -o \"$fileNameWithoutExt\" && ./$fileNameWithoutExt"
  },
  "[scss]": {
    "editor.tabSize": 4
  }
}
